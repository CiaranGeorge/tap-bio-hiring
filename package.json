{
  "name": "weatherify",
  "version": "0.1.0",
  "config": {
    "stack_name": "weatherify"
  },
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "date-fns": "^2.0.0-alpha.8",
    "material-ui-pickers": "^1.0.0-rc.9",
    "moment": "^2.22.1",
    "node-sass-chokidar": "^1.3.0",
    "npm": "^5.8.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "react-scripts": "^1.1.0",
    "redux-devtools": "^3.4.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar public/ -o public/",
    "watch-css": "npm run build-css && node-sass-chokidar public/ -o public/ --watch --recursive",
    "start": "react-scripts start",
    "deploy": "npm run build && aws s3 cp ./build s3://$npm_package_config_stack_name --recursive",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
